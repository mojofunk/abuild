#!/bin/bash

function set_pkg_env
{
	PKG_NAME="pangomm"
	PKG_VERSION="2.34.0"

	PKG_SRC_DIR="${PKG_NAME}-${PKG_VERSION}"
	PKG_SRC_FILE="${PKG_SRC_DIR}.tar.xz"
	PKG_SRC_URL="http://ftp.gnome.org/pub/GNOME/sources/pangomm/2.34/${PKG_SRC_FILE}"

	PKG_DEPS="pango cairomm glibmm libsigc++"

	PKG_BUILD_TOOL="autotools"
}
