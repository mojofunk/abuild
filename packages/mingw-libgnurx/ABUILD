#!/bin/bash

function set_pkg_env
{
	PKG_NAME="mingw-libgnurx"
	PKG_VERSION="2.5.1"

	PKG_SRC_DIR="${PKG_NAME}-${PKG_VERSION}"
	PKG_SRC_FILE="${PKG_SRC_DIR}-src.tar.gz"
	PKG_SRC_URL="http://sourceforge.net/projects/mingw/files/Other/UserContributed/regex/mingw-regex-2.5.1/${PKG_SRC_FILE}"

	PKG_BUILD_TOOL="autotools"
}
